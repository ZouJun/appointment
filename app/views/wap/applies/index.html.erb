<div class="weui-grids">
  <% @categories.life.each do |f| %>
      <a href="<%= ordering_wap_applies_path(category_id: f.id) %>" class="weui-grid">
        <div class="weui-grid__icon">
          <img src="/wap/image/icon_nav_button.png" alt="">
        </div>
        <p class="weui-grid__label">
          <%= f.name %>
        </p>
      </a>
  <% end %>

  <% @categories.service.each do |f| %>
      <a href="<%= ordering_wap_applies_path(category_id: f.id) %>" class="weui-grid">
        <div class="weui-grid__icon">
          <img src="/wap/image/icon_nav_cell.png" alt="">
        </div>
        <p class="weui-grid__label">
          <%= f.name %>
        </p>
      </a>
  <% end %>
</div>
<%=  render 'layouts/wap_footer' %>