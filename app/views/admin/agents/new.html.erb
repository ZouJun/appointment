<% content_for :content_for do %>
    <ol class="breadcrumb">
      <li><a href="#">代理商管理</a></li>
      <li class="active">新增</li>
    </ol>

    <div class="add_agent">
    <%= form_for @agent, url: admin_agents_path do |f| %>
        <div class="form-group">
          <%= f.text_field :name, class: "form-control", id: "exampleInputEmail1", placeholder: "用户名" %>
          <!--<%#= f.text_field　:name, class: "form-control", id: "exampleInputEmail1", placeholder: "用户名" %>-->
        </div>
        <div class="form-group">
          <%= f.text_field :login, class: "form-control", id: "exampleInputEmail1", placeholder: "登陆账号" %>
        </div>
        <div class="form-group">
          <%= f.password_field :password, class: "form-control", id: "exampleInputEmail1", placeholder: "登陆密码" %>
        </div>
          <div class="form-group">
            <%= f.select :proxy_type, Agent.proxy_type_options, class: "form-control", id: "exampleInputEmail1" %>
          </div>
        <div class="form-group">
          <%= f.text_field :email, class: "form-control", id: "exampleInputEmail1", placeholder: "电子邮箱" %>
        </div>
        <div class="form-group">
          <%= f.text_field :mobile, class: "form-control", id: "exampleInputEmail1", placeholder: "联系方式" %>
        </div>
        <div class="form-group">
          <%= f.text_field :address, class: "form-control", id: "exampleInputEmail1", placeholder: "地址" %>
        </div>
        <div class="form-group">
          <%= f.text_area :description, class: "form-control", id: "exampleInputEmail1", placeholder: "代理商描述" %>
        </div>
        <button type="submit" class="btn btn-default">提交</button>
    <% end %>
</div>
<% end %>