<!DOCTYPE html>
<html>
<head>
  <title>Appointment</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render partial: '/layouts/head' %>

<%= render partial: '/layouts/left_partial' %>
<div class="right">
  <%= yield :content_for %>
</div>

<% if flash[:notice] %>
    <%= flash[:notice] %>
<% elsif flash[:alert] %>
    <%= flash[:alert] %>
<% end %>
<% flash.clear %>
</body>
</html>